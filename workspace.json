{
  "version": 1,
  "projects": {
    "app1": {
      "projectType": "application",
      "root": "apps/app1",
      "sourceRoot": "apps/app1/src",
      "architect": {
        "build": {
          "builder": "@nrwl/gatsby:build"
        },
        "develop": {
          "builder": "@nrwl/gatsby:develop"
        }
      }
    },
    "app2": {
      "root": "apps/app2",
      "sourceRoot": "apps/app2/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/app2",
            "index": "apps/app2/src/index.html",
            "main": "apps/app2/src/main.tsx",
            "polyfills": "apps/app2/src/polyfills.ts",
            "tsConfig": "apps/app2/tsconfig.app.json",
            "assets": ["apps/app2/src/favicon.ico", "apps/app2/src/assets"],
            "styles": [],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/app2/src/environments/environment.ts",
                  "with": "apps/app2/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "app2:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "app2:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/app2/.eslintrc",
            "tsConfig": [
              "apps/app2/tsconfig.app.json",
              "apps/app2/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/app2/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/app2/jest.config.js",
            "tsConfig": "apps/app2/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "app2-e2e": {
      "root": "apps/app2-e2e",
      "sourceRoot": "apps/app2-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/app2-e2e/cypress.json",
            "tsConfig": "apps/app2-e2e/tsconfig.e2e.json",
            "devServerTarget": "app2:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "app2:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/app2-e2e/.eslintrc",
            "tsConfig": ["apps/app2-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/app2-e2e/**"]
          }
        }
      }
    },
    "app3": {
      "root": "apps/app3",
      "sourceRoot": "apps/app3/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/app3",
            "index": "apps/app3/src/index.html",
            "main": "apps/app3/src/main.js",
            "polyfills": "apps/app3/src/polyfills.js",
            "tsConfig": "apps/app3/tsconfig.app.json",
            "assets": ["apps/app3/src/favicon.ico", "apps/app3/src/assets"],
            "styles": [],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/app3/src/environments/environment.js",
                  "with": "apps/app3/src/environments/environment.prod.js"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "app3:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "app3:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/app3/.eslintrc",
            "tsConfig": [
              "apps/app3/tsconfig.app.json",
              "apps/app3/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/app3/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/app3/jest.config.js",
            "tsConfig": "apps/app3/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "app3-e2e": {
      "root": "apps/app3-e2e",
      "sourceRoot": "apps/app3-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/app3-e2e/cypress.json",
            "tsConfig": "apps/app3-e2e/tsconfig.e2e.json",
            "devServerTarget": "app3:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "app3:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/app3-e2e/.eslintrc",
            "tsConfig": ["apps/app3-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/app3-e2e/**"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/gatsby"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "eslint"
      }
    },
    "@nrwl/react": {
      "application": {
        "style": "styled-components",
        "linter": "eslint",
        "babel": true
      },
      "library": {
        "style": "styled-components",
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      },
      "component": {
        "style": "styled-components"
      }
    },
    "@nrwl/next": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/web": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/nx-plugin": {
      "plugin": {
        "linter": "eslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/gatsby": {
      "application": {}
    }
  },
  "defaultProject": "app2"
}
